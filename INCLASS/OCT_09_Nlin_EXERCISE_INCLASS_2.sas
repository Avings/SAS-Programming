DATA NLIN_EX;
INPUT X Y;
DATALINES;
4 0.2
6 0.7
8 1.4
10 2.8
12 5.8
14 11.1
16 21.7
18 43.2
20 80.1
;
PROC Nlin;
PARAMETERS a = 0.124 b = 0.3238;
MODEL y = a*exp(b*x);
*OUTPUT OUT=predexpdata PREDICTED= PREDY;
OUTPUT OUT=predexpdata RESIDUAL= PREDR;
RUN;
SYMBOL1 VALUE = 'CIRCLE' COLOR='BLUE';
SYMBOL2 COLOR='ORANGE' I=JOIN;
PROC GPLOT DATA=PREDEXPDATA;*PREDICTED;
PLOT y*x = 1
PREDY*x = 2/overlay;
run; 
PROC GPLOT DATA=PREDEXPDATA;*RESIDUALS;
PLOT PREDR*X/VREF=0;
RUN;
